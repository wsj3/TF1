{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "devCommand": "npm run dev",
  "ignoreCommand": null,
  "git": {
    "deploymentEnabled": {
      "main": true,
      "staging": true,
      "development": false,
      "feature/*": false
    }
  },
  "outputDirectory": ".next",
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/minimal", "dest": "/minimal.js" }
  ]
} 