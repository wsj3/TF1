{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "devCommand": "npm run dev",
  "ignoreCommand": null,
  "build": {
    "env": {
      "CACHE_KEY": "reset-20241002-5",
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_ENV": "production"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "staging": true,
      "development": false,
      "feature/*": false
    }
  },
  "outputDirectory": ".next",
  "functions": {
    "pages/api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": []
} 